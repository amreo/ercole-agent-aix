distribution
product
    tag ercole-agent
    control_directory ""
    revision $VERSION
fileset
    tag files
    control_directory ""
    is_locatable false
    state 0
    tile ercole-agent
    directory fetch /opt/ercole-agent/fetch
    file *
    directory config /opt/ercole-agent/config
    file *
    directory lib /opt/ercole-agent/lib
    file *
    directory marshal /opt/ercole-agent/marshal
    file *
    file_permissions -o root -g root
